<launch>

    <node name="wheel_pid" pkg="pid" type="controller" ns="front_left_wheel">
      <param name="Kp" value="5.0" />
      <param name="Ki" value="0.0" />
      <param name="Kd" value="0.0" />
      <param name="upper_limit" value="100" />
      <param name="lower_limit" value="0" />
      <param name="windup_limit" value="5" />
      <param name="max_loop_frequency" value="15.0" />
      <param name="min_loop_frequency" value="0.0" />
    </node>


  <node pkg="i2cpwm_board" name="i2cpwm_board" type="i2cpwm_board">
  </node>

  <node pkg="toulouse_rover" name="robot_control_node" type="robot_control_node"  output="screen"> </node>

</launch>
