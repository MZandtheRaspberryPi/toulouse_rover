<launch>
  <test test-name="test_wheel_speed_controller_pid_node" pkg="toulouse_rover" type="test_wheel_speed_controller_pid_node" time-limit="7.0"/>

  <node name="back_left_wheel_pid" pkg="pid" type="controller" ns = "back_left_wheel_pid_ns" output="screen">
      <param name="Kp" value="100.0" />
      <param name="Ki" value="0.0" />
      <param name="Kd" value="0.1" />
      <param name="upper_limit" value="4000" />
      <param name="lower_limit" value="900" />
      <param name="windup_limit" value="10" />
      <param name="max_loop_frequency" value="100.0" />
      <param name="min_loop_frequency" value="0" />
       <param name="min_loop_frequency" value="-1" />
    </node>

    <node name="back_right_wheel_pid" pkg="pid" type="controller" ns = "back_right_wheel_pid_ns" output="screen">
      <param name="Kp" value="100.0" />
      <param name="Ki" value="0.0" />
      <param name="Kd" value="0.1" />
      <param name="upper_limit" value="4000" />
      <param name="lower_limit" value="900" />
      <param name="windup_limit" value="10" />
      <param name="max_loop_frequency" value="100.0" />
      <param name="min_loop_frequency" value="0" />
       <param name="min_loop_frequency" value="-1" />
    </node>
</launch>